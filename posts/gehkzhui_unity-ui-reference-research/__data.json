this with the scale animation from before.",[],{"id":583,"order":584,"childrenIds":585,"style":586,"componentAttr":587},"683dc0b3e212b50bbe5eca7c",46,[],{"backgroundColor":16,"align":65,"verticalAlign":66},{"componentType":263,"orginal_type":264,"style":265},{"id":45,"order":589,"childrenIds":590,"style":591,"componentAttr":592},47,[],{"backgroundColor":16,"align":65,"verticalAlign":66},{"componentType":68,"order":9,"text":46,"style":96,"marks":593},[],{"id":595,"order":596,"fields":597,"childrenIds":599,"style":600,"componentAttr":601},"683dc0b3e212b50bbe5eca7e",48,{"isUnwrapped":598,"lang":84},true,[],{"backgroundColor":16,"align":65,"verticalAlign":66},{"componentType":68,"order":9,"text":602,"style":89,"marks":603},"\nusing UnityEngine;\nusing UnityEngine.UIElements;\nusing System.Collections;\n\npublic class AnimatedSpriteButton : MonoBehaviour\n{\n    public Texture2D[] animationFrames; // Assign in Inspector: frames of your sprite animation\n    public float frameRate = 10f;       // Frames per second for sprite animation\n\n    private VisualElement root;\n    private Button animatedButton;\n    private Coroutine spriteAnimationCoroutine;\n\n    void OnEnable()\n    {\n        var uiDocument = GetComponent\u003CUIDocument>();\n        root = uiDocument.rootVisualElement;\n\n        animatedButton = new Button(() => Debug.Log(\"Button clicked\"))\n        {\n            text = \"Click Me\"\n        };\n\n        // Set initial style for button size and background\n        animatedButton.style.width = 200;\n        animatedButton.style.height = 60;\n        animatedButton.style.unityTextAlign = TextAnchor.MiddleCenter;\n        animatedButton.style.backgroundImage = animationFrames.Length > 0 ? new StyleBackground(animationFrames[0]) : StyleBackground.none;\n\n        root.Add(animatedButton);\n\n        // Start sprite animation coroutine\n        if (animationFrames.Length > 0)\n        {\n            spriteAnimationCoroutine = StartCoroutine(AnimateSpriteBackground(animatedButton));\n        }\n\n        // Register pointer events for scale animation\n        animatedButton.RegisterCallback\u003CPointerEnterEvent>(evt => AnimateScale(animatedButton, 1.1f));\n        animatedButton.RegisterCallback\u003CPointerLeaveEvent>(evt => AnimateScale(animatedButton, 1.0f));\n        animatedButton.RegisterCallback\u003CPointerDownEvent>(evt => AnimateScale(animatedButton, 0.9f));\n        animatedButton.RegisterCallback\u003CPointerUpEvent>(evt => AnimateScale(animatedButton, 1.1f));\n    }\n\n    private IEnumerator AnimateSpriteBackground(VisualElement element)\n    {\n        int frameCount = animationFrames.Length;\n        int currentFrame = 0;\n        float frameTime = 1f / frameRate;\n\n        while (true)\n        {\n            element.style.backgroundImage = new StyleBackground(animationFrames[currentFrame]);\n            currentFrame = (currentFrame + 1) % frameCount;\n            yield return new WaitForSeconds(frameTime);\n        }\n    }\n\n    private void AnimateScale(VisualElement element, float targetScale)\n    {\n        StartCoroutine(ScaleCoroutine(element, targetScale, 0.2f));\n    }\n\n    private IEnumerator ScaleCoroutine(VisualElement element, float targetScale, float duration)\n    {\n        float time = 0f;\n        float startScale = element.transform.scale.x;\n        while (time \u003C duration)\n        {\n            time += Time.deltaTime;\n            float scale = Mathf.Lerp(startScale, targetScale, time / duration);\n            element.transform.scale = new Vector3(scale, scale, 1);\n            yield return null;\n        }\n        element.transform.scale = new Vector3(targetScale, targetScale, 1);\n    }\n\n    void OnDisable()\n    {\n        if (spriteAnimationCoroutine != null)\n        {\n            StopCoroutine(spriteAnimationCoroutine);\n        }\n    }\n}\n\n",[],{"id":605,"order":606,"childrenIds":607,"style":608,"componentAttr":609},"683dc0b3e212b50bbe5eca7f",49,[],{"bac